{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/Shahin_Comprehensive_Review_Index.html"
    },
    {
      "source": "/index",
      "destination": "/Shahin_Comprehensive_Review_Index.html"
    },
    {
      "source": "/main",
      "destination": "/Shahin_Comprehensive_Review_Index.html"
    },
    {
      "source": "/analysis",
      "destination": "/Shahin_Comprehensive_Review_Index.html"
    },
    {
      "source": "/architecture",
      "destination": "/Shahin_Architecture_Platform_Comprehensive.html"
    },
    {
      "source": "/market",
      "destination": "/Saudi_Market_Regulations_Requirements_Comprehensive.html"
    },
    {
      "source": "/grc",
      "destination": "/GRC_Problems_Gaps_Solutions_KSA_Comprehensive.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}